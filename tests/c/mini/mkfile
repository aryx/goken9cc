%.exe: %.c
   6c -c $prereq
   6a -c write.s
   6a -c exit.s
   gopack grc libmini.a write.6 exit.6
   # -X to require NOSPLIT for all linked code, especially in $stem.6
   6l -X -L. -o $target -E main $stem.6
