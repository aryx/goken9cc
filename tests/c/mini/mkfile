# 6a -c morestack.s
helloc.exe: helloc.c
   6c -c $prereq
   6a -c xwrite.s
   6a -c xexit.s
   gopack grc libmini.a xwrite.6 xexit.6
   # -X to require NOSPLIT for all linked code, especially in $stem.6
   6l -X -L. -o $target -E main helloc.6
