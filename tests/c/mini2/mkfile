
all:V: helloprintf.exe

clean:V:
	rm -f *.6 *.exe *.a

# can use -v for verbose mode to help debug
helloprintf.exe: helloprintf.c
   6c -c misc.c
   6c -c print.c
   6c -c float.c
   6c -c helloprintf.c
   6a -c linux.s
   gopack grc libprint.a linux.6 print.6 float.6 misc.6
   6l -X -L. -o $target -E main helloprintf.6
